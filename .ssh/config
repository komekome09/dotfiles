	HostName sol.edu.cc.uec.ac.jp
	User h1313144
	IdentityFile ~/.ssh/sol_id_rsa
	TCPKeepAlive yes
	IdentitiesOnly yes
	LocalForward 8080 proxy.uec.ac.jp:8080

